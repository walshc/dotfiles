
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
! URxvt
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
URxvt.letterSpace: -1
URxvt*font: xft:Inconsolata:size=16,xft:Inconsolata for Powerline:size=16
URxvt*boldFont: xft:Inconsolata:size=16:bold,xft:Inconsolata for Powerline:size=16:bold
URxvt*italicFont: xft:Inconsolata:size=16:italic,xft:Inconsolata for Powerline:size=16:italic
URxvt*bolditalicFont: xft:Inconsolata:size=16:bolditalic,xft:Inconsolata for Powerline:size=16:bolditalic
!URxvt*font: xft:Inconsolata:size=22,xft:Inconsolata for Powerline:size=22
!URxvt*boldFont: xft:Inconsolata:size=22:bold,xft:Inconsolata for Powerline:size=22:bold
!URxvt*italicFont: xft:Inconsolata:size=22:italic,xft:Inconsolata for Powerline:size=22:italic
!URxvt*bolditalicFont: xft:Inconsolata:size=22:bolditalic,xft:Inconsolata for Powerline:size=22:bolditalic
URxvt.urlLauncher: /usr/bin/google-chrome
URxvt.matcher.button: 1
URxvt*geometry: 85x24
URxvt*scrollBar: false
URxvt.perl-ext-common: default,clipboard,font-size,fullscreen
URxvt.keysym.C-Up:   perl:font-size:increase
URxvt.keysym.C-Down: perl:font-size:decrease
URxvt.font-size.step: 6
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-F11: perl:fullscreen:switch
URxvt.iso14755: False
urxvt*blurRadius: 50
URxvt.depth: 32
! URxvt.background: [85]#2d2d2d
!URxvt.background: [85]#404552
! URxvt.background: [95]#404552
URxvt*iconFile: /usr/share/icons/Numix-Circle/48x48/apps/Terminal.svg

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
! Use extended color scheme
rofi.color-enabled:                  true
! Color scheme for normal row
rofi.color-normal:                   #ffffff,#525d76,#f5f5f5,#5294e2,#ffffff
! Color scheme for urgent row
rofi.color-urgent:                   #ffffff,#dc322f,#f5f5f5,#5294e2,#dc322f
! Color scheme for active row
rofi.color-active:                   #ffffff,#c2cad0,#f5f5f5,#5294e2,#c2cad0
! Color scheme window
rofi.color-window:                   #ffffff,#f5f5f5
! Separator style (none, dash, solid)
rofi.separator-style:                solid

